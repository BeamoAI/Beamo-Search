export bing_video_search_api_key2="REPLACE WITH YOUR OWN BING API KEY"
export bing_news_search_api_key1="REPLACE WITH YOUR OWN BING API KEY"
export bing_news_search_api_key2="REPLACE WITH YOUR OWN BING API KEY"
export bing_web_search_api_key1="REPLACE WITH YOUR OWN BING API KEY"
export bing_web_search_api_key2="REPLACE WITH YOUR OWN BING API KEY"
# END SECTION OF BING API KEYS

export ANYSCALE_ENDPOINT_API_KEY="REPLACE WITH YOUR OWN ANYSCALE ENDPOINT API KEY"
