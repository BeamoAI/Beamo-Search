# These variables are for the Apache runtime environment

export APACHE_RUN_DIR=/var/run/apache2
export APACHE_LOG_DIR=/var/log/apache2
export APACHE_RUN_USER=www-data
export APACHE_RUN_GROUP=www-data
export APACHE_PID_FILE=$APACHE_RUN_DIR/apache2.pid

# EDIT EVERYTHING BELOW TO BE YOUR OWN API KEYS
export claude_key="REPLACE WITH YOUR OWN ANTHROPIC API KEY"

# You can use the same Bing API key for all of these below in this section,
# we just have separate values in order to save costs on certain search types
# and you can do the same with different keys in different pricing tiers

